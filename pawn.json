{
    "user": "urShadow",
    "repo": "Pawn.Regex",
    "include_path": "src",
    "resources": [
        {
            "name": "^pawnregex-(.*)-linux.tar.gz$",
            "platform": "linux",
            "archive": true,
            "plugins": ["pawnregex.so"]
        },
        {
            "name": "^pawnregex-(.*)-win32.zip$",
            "platform": "windows",
            "archive": true,
            "plugins": ["pawnregex.dll"]
        }
    ],
    "runtime": {
        "plugins": ["urShadow/pawnregex"]
    }
}